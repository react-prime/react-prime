{
  "name": "react-prime",
  "version": "6.1.0",
  "description": "A starter kit to create comprehensive React apps with Redux",
  "author": "Label A",
  "url": "https://labela.nl/",
  "repository": {
    "type": "git",
    "url": "https://github.com/react-prime/react-prime"
  },
  "license": "MIT",
  "scripts": {
    "start": "TS_NODE_PROJECT=config/tsconfig.json webpack serve --config config/webpack.config.dev.ts",
    "build": "rimraf dist/ && npm run build:server && TS_NODE_PROJECT=config/tsconfig.json webpack --progress --config config/webpack.config.prod.ts",
    "build:server": "tsc --project server/tsconfig.json",
    "server": "node dist/server.js",
    "clean": "rimraf dist/",
    "lint": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx src/",
    "stylelint": "stylelint './src/**/*.ts*'",
    "analyzer": "webpack --config config/webpack.config.prod.ts --profile --json > stats.json && webpack-bundle-analyzer stats.json ./dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "webpack",
    "redux",
    "boilerplate",
    "babel",
    "styled-components",
    "typescript"
  ],
  "browserslist": [
    "> 0.2%",
    "not dead",
    "not op_mini all"
  ],
  "dependencies": {
    "compression": "1.7.4",
    "express": "4.17.1",
    "lodash": "4.17.21",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.4",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "redux": "4.1.0",
    "redux-thunk": "2.3.0",
    "styled-components": "5.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.0",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-export-default-from": "7.12.13",
    "@babel/plugin-proposal-export-namespace-from": "7.12.13",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.14.1",
    "@babel/preset-react": "7.13.13",
    "@babel/preset-typescript": "7.13.0",
    "@svgr/webpack": "5.5.0",
    "@types/compression": "1.7.0",
    "@types/copy-webpack-plugin": "8.0.0",
    "@types/html-webpack-plugin": "3.2.5",
    "@types/react": "17.0.5",
    "@types/react-dom": "17.0.3",
    "@types/react-redux": "7.1.16",
    "@types/react-router": "5.1.14",
    "@types/react-router-dom": "5.1.7",
    "@types/styled-components": "5.1.9",
    "@types/webpack": "5.28.0",
    "@types/webpack-dev-server": "3.11.3",
    "@types/workbox-webpack-plugin": "5.1.6",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "babel-loader": "8.2.2",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-styled-components": "1.12.0",
    "copy-webpack-plugin": "8.1.1",
    "css-loader": "5.2.4",
    "eslint": "7.25.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.7.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "6.2.0",
    "fork-ts-checker-webpack-plugin": "6.2.6",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "rimraf": "3.0.2",
    "style-loader": "2.0.0",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "terser-webpack-plugin": "5.1.1",
    "ts-node": "9.1.1",
    "tsconfig-paths-webpack-plugin": "3.5.1",
    "typesafe-actions": "5.1.0",
    "typescript": "4.2.4",
    "url-loader": "4.1.1",
    "webpack": "5.36.2",
    "webpack-bundle-analyzer": "4.4.1",
    "webpack-cli": "4.7.0",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "5.7.3",
    "workbox-webpack-plugin": "6.1.5"
  }
}
