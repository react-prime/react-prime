{
  "name": "react-prime",
  "version": "5.2.0",
  "description": "A starter kit to create comprehensive React apps with Redux and serverside rendering.",
  "author": "jornbostelaar",
  "repository": {
    "type": "git",
    "url": "https://github.com/JBostelaar/react-prime"
  },
  "license": "MIT",
  "scripts": {
    "start": "node devServer.js",
    "build": "rimraf dist/ && webpack -p --hide-modules --config webpack.config.prod.js",
    "server": "node dist/server.js",
    "clean": "rimraf dist/",
    "lint": "eslint --ext .jsx --ext .js src/",
    "analyzer:build": "NODE_ENV=production webpack -p --config webpack.config.prod.js --profile --json > stats.json",
    "analyzer:run": "webpack-bundle-analyzer stats.json ./dist",
    "analyzer": "run-s analyzer:build analyzer:run"
  },
  "keywords": [
    "react",
    "webpack",
    "express",
    "redux",
    "boilerplate",
    "babel",
    "styled-components"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "compression": "^1.7.2",
    "express": "^4.16.3",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "styled-components": "^3.2.5",
    "svg-react-loader": "^0.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-styled-components": "^1.5.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.1",
    "eslint-config-react-app": "^3.0.3",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.1.2",
    "webpack-hot-middleware": "^2.22.0",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "engines": {
    "node": "8.9.1"
  }
}
