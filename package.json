{
  "name": "react-prime",
  "version": "7.0.0",
  "description": "A starter kit to create comprehensive React apps with React Query",
  "author": "Label A",
  "url": "https://labela.nl/",
  "repository": {
    "type": "git",
    "url": "https://github.com/react-prime/react-prime"
  },
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development TS_NODE_PROJECT=config/tsconfig.json webpack serve --config config/webpack.config.dev.ts",
    "build": "rimraf dist/ && NODE_ENV=production npm run build:server && NODE_ENV=production TS_NODE_PROJECT=config/tsconfig.json webpack --progress --config config/webpack.config.prod.ts",
    "build:server": "tsc --project server/tsconfig.json",
    "server": "node dist/server.js",
    "clean": "rimraf dist/",
    "lint": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx src/",
    "stylelint": "stylelint './src/**/*.ts*'",
    "analyzer": "webpack --config config/webpack.config.prod.ts --profile --json > stats.json && webpack-bundle-analyzer stats.json ./dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "webpack",
    "react-query",
    "boilerplate",
    "babel",
    "styled-components",
    "typescript"
  ],
  "browserslist": [
    "> 0.2%",
    "not dead",
    "not op_mini all"
  ],
  "dependencies": {
    "compression": "1.7.4",
    "express": "4.17.1",
    "lodash": "4.17.21",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "3.34.0",
    "react-router": "6.0.2",
    "react-router-dom": "6.0.2",
    "styled-components": "5.3.3"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/eslint-parser": "7.16.3",
    "@babel/plugin-proposal-class-properties": "7.16.0",
    "@babel/plugin-proposal-export-default-from": "7.16.0",
    "@babel/plugin-proposal-export-namespace-from": "7.16.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-react": "7.16.0",
    "@babel/preset-typescript": "7.16.0",
    "@svgr/webpack": "6.0.0",
    "@types/compression": "1.7.2",
    "@types/copy-webpack-plugin": "8.0.1",
    "@types/html-webpack-plugin": "3.2.6",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@types/react-router": "5.1.17",
    "@types/react-router-dom": "5.3.2",
    "@types/styled-components": "5.1.16",
    "@types/webpack": "5.28.0",
    "@types/webpack-dev-server": "4.5.0",
    "@types/workbox-webpack-plugin": "5.1.8",
    "@typescript-eslint/eslint-plugin": "5.5.0",
    "@typescript-eslint/parser": "5.5.0",
    "babel-loader": "8.2.3",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-styled-components": "2.0.2",
    "copy-webpack-plugin": "10.0.0",
    "css-loader": "6.5.1",
    "eslint": "8.3.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "file-loader": "6.2.0",
    "fork-ts-checker-webpack-plugin": "6.5.0",
    "html-webpack-plugin": "5.5.0",
    "rimraf": "3.0.2",
    "style-loader": "3.3.1",
    "stylelint": "14.1.0",
    "stylelint-config-standard": "24.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "terser-webpack-plugin": "5.2.5",
    "ts-node": "10.4.0",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "typesafe-actions": "5.1.0",
    "typescript": "4.5.2",
    "url-loader": "4.1.1",
    "webpack": "5.64.4",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.6.0",
    "webpack-merge": "5.8.0",
    "workbox-webpack-plugin": "6.4.1"
  }
}
