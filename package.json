{
  "name": "react-prime",
  "version": "6.0.0",
  "description": "A starter kit to create comprehensive React apps with Redux",
  "author": "jornbostelaar",
  "repository": {
    "type": "git",
    "url": "https://github.com/react-prime/react-prime"
  },
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.config.dev.ts",
    "build": "rimraf dist/ && npm run build:server && webpack -p --hide-modules --config config/webpack.config.prod.ts",
    "build:server": "tsc --project server/tsconfig.json",
    "server": "node dist/server.js",
    "clean": "rimraf dist/",
    "lint": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx src/",
    "stylelint": "stylelint './src/**/*.js*'",
    "analyzer": "webpack -p --config config/webpack.config.prod.ts --profile --json > stats.json && webpack-bundle-analyzer stats.json ./dist"
  },
  "keywords": [
    "react",
    "webpack",
    "express",
    "redux",
    "boilerplate",
    "babel",
    "styled-components",
    "typescript"
  ],
  "browserslist": {
    "production": [
      "> 0.2%",
      "ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "lodash": "4.17.15",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "^7.2.0",
    "react-router": "5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "5.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@babel/plugin-proposal-class-properties": "7.10.1",
    "@babel/plugin-proposal-export-default-from": "7.10.1",
    "@babel/plugin-proposal-export-namespace-from": "7.10.1",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.10.2",
    "@babel/preset-react": "7.10.1",
    "@babel/preset-typescript": "7.10.1",
    "@svgr/webpack": "5.4.0",
    "@types/compression": "1.7.0",
    "@types/copy-webpack-plugin": "6.0.0",
    "@types/html-webpack-plugin": "3.2.3",
    "@types/react": "16.9.36",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.9",
    "@types/react-router": "5.1.7",
    "@types/react-router-dom": "5.1.5",
    "@types/styled-components": "5.1.0",
    "@types/webpack": "4.41.17",
    "@types/webpack-dev-server": "3.11.0",
    "@types/webpack-merge": "4.1.5",
    "@types/workbox-webpack-plugin": "5.1.1",
    "@typescript-eslint/eslint-plugin": "3.2.0",
    "@typescript-eslint/parser": "3.2.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-styled-components": "1.10.7",
    "copy-webpack-plugin": "6.0.2",
    "css-loader": "3.6.0",
    "eslint": "7.2.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-flowtype": "5.1.3",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "file-loader": "6.0.0",
    "fork-ts-checker-webpack-plugin": "5.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "rimraf": "3.0.2",
    "style-loader": "1.2.1",
    "stylelint": "13.6.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "terser-webpack-plugin": "3.0.4",
    "ts-node": "8.10.2",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "tslint": "6.1.2",
    "typesafe-actions": "5.1.0",
    "typescript": "3.9.5",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "^4.2.2",
    "workbox-webpack-plugin": "5.1.3"
  }
}
